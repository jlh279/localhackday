{
  "manifest_version": 2,
  "name": "The Wikipedia Game",
  "version": "0.1",
  "background": {
    "scripts": ["js/background.js"]
  },
  "permission": [
      "tabs",
      "storage"
  ]
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["js/jquery-3.1.1.min.js", "js/content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
	"default_popup": "popup.html"
  }
}
